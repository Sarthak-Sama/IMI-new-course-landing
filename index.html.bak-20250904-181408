    <!-- Custom Script -->
    <!-- Refactoring Content -->
        <script>
      document.addEventListener("DOMContentLoaded", function () {
        const startDateValue = "October 2025";
        const programmeFeeValue = "4,36,000";
        
        // Build the Loan Section
          function buildLoanSection(){          
            const footerText = document.querySelector(".registration_registration__paymentOption")
            footerText.innerHTML = `<p><b>Spots are filling fast  don't let "later" turn into "too late"</b><br/><i>Flexible payment options make it easier to say yes.</i></p><br/>`

            const stickyBarDurationText = document.querySelector(".text-color.info-bar_info-bar__duration");
            stickyBarDurationText.innerHTML = "2 years (with 2 Campus Immersion)<br/>Weekly synchronous Live sessions"

            const partnerDiv = document.createElement("div");
            const partnerDivText = document.createElement("p");

            partnerDivText.innerText = "Strategic Partner";
            partnerDivText.style.textAlign = "center";

            const partnerDivImg = document.createElement("img");
            partnerDivImg.src = "./media/partner.png";
            partnerDivImg.style.width = "200px";

            partnerDiv.appendChild(partnerDivText);
            partnerDiv.appendChild(partnerDivImg);

            footerText.appendChild(partnerDiv);
            
            const careerServicesHeading = document.querySelector("#icons-text-career-services--offered-by-imi-new-delhi h2");
            console.log(careerServicesHeading);
            careerServicesHeading.innerText = "Career Services offered by IMI Delhi";
        }

        // Update service icons

        function updateServiceIcons(){


          const phItems = document.querySelectorAll(".ph-item");
            phItems.forEach((item) => {
                item.style.width = "20%";
            });

            
            const serviceSection = document.querySelector("#icons-text-career-services--offered-by-imi-new-delhi");
            const serviceIconsRows = serviceSection.querySelectorAll(".row");

            const executiveCoachingIconDiv  = serviceIconsRows[0].querySelector(".ph-item");

            const divsToMoveFromRow2toRow1 = [serviceIconsRows[1].querySelector(".ph-item:nth-child(1)")];
            divsToMoveFromRow2toRow1.forEach(div => {
              serviceIconsRows[0].appendChild(div);
            });

            const divsToMoveFromRow3toRow2 = serviceIconsRows[2].querySelectorAll(".ph-item");
            divsToMoveFromRow3toRow2.forEach(div => {
              serviceIconsRows[1].appendChild(div);
            });
        }

        // Function to modify the DOM
        function updateContent() {
          try {

              const batchTag = document.querySelector(
                "p.m-0.overline--one"
              );
              if(batchTag){
                batchTag.style.fontWeight = "bold";
              }


             const detailElems = document.querySelectorAll(
            ".info-bar_info-bar__buildingBlock"
          );
          const stickyHeaderTitleElems = document.querySelectorAll(
            ".text-color.info-bar_info-bar__labelText"
          );
          stickyHeaderTitleElems.forEach((element) => {
            element.style.backgroundColor = "#e6903a";
            element.style.setProperty("color", "#ffffff", "important");
            element.style.width = "fit-content";
            element.style.padding = "0.2rem 0.5rem";
          });
    
            // Only proceed if elements are found
            if (detailElems && detailElems.length > 0) {
              const startDateElem = detailElems[0].getElementsByTagName("p")[1];
              if (startDateElem) startDateElem.outerHTML = `<p>Session starts <b>${startDateValue}</b></p>`;
    
              const programmeFeeElem = detailElems[2].getElementsByTagName("p")[1];
              if (programmeFeeElem) {
                programmeFeeElem.style.fontWeight = "bold";
                programmeFeeElem.style.marginTop = "0.2rem";
    
                // Set font size based on screen width
                function updateFontSize() {
                  if (window.innerWidth <= 991) {
                    programmeFeeElem.style.fontSize = "1rem";
                  } else {
                    programmeFeeElem.style.fontSize = "1.125rem";
                  }
                }
    
                // Set initial font size
                updateFontSize();
    
                // Update font size on window resize
                window.addEventListener("resize", updateFontSize);
    
                programmeFeeElem.innerText = programmeFeeValue;
    
                const programmeFeeNote = document.createElement("p");
                programmeFeeNote.innerHTML =
                  "<p>Note* - <b>EMI plans are available</b></p>";
                programmeFeeNote.style.fontSize = "0.875rem";
                programmeFeeNote.style.marginTop = "-0.8rem";
                programmeFeeElem.parentElement.appendChild(programmeFeeNote);
              }
    
              // Updating footer
              const footerElem = document.querySelector(
                ".row.d-flex.justify-content-between.text-center.text-white"
              );
              if (footerElem) {
                const elementToRemove = footerElem.getElementsByTagName("p")[1];
                if (elementToRemove) {
                  elementToRemove.remove();
                }
              }
    
              // Updating the Certificate
              const certificateContainer = document.querySelector(
                ".col-xl-5.col-lg-6.d-flex.align-items-center.image-cntr.justify-content-center"
              );
              if (certificateContainer) {
                const certificateImgElem = certificateContainer.querySelector("img");
                if (certificateImgElem) {
                  certificateImgElem.removeAttribute("srcset");
                  certificateImgElem.src = "./media/sample-certificate.png";
                }
              }
    
              const newBtn = document.querySelector(
                ".npfWidgetButton.npfWidget-5f18efeb970d645a64e55f82f8662afb"
              );

              buildLoanSection();
              updateServiceIcons();
              
              // If we found at least some elements, we can consider our update successful
              return true;
            }
          } catch (err) {
          }
          
          // If we reach here, the update wasn't successful
          return false;
        }
        
        // For Next.js, we can try to use the window.__NEXT_DATA__ object to detect when the app has hydrated
        function checkNextLoaded() {
          if (window.__NEXT_DATA__ && document.readyState === 'complete') {
            return true;
          }
          return false;
        }
    
        // Main execution logic with retry mechanism
        function attemptUpdate() {
          // Try immediate update
          if (updateContent()) {
            return;
          }
          
          // If Next.js is loaded, try again
          if (checkNextLoaded()) {
            // Give a tiny delay for the React components to render fully
            setTimeout(updateContent, 50);
            return;
          }
          
          // Set up MutationObserver as a fallback
          const observer = new MutationObserver((mutations, obs) => {
            // Check if required elements exist now
            if (updateContent()) {
              obs.disconnect(); // Stop observing once updated
            } else if (checkNextLoaded()) {
              // If Next.js loaded but content not updated yet, try once more and disconnect
              setTimeout(() => {
                updateContent();
                obs.disconnect();
              }, 50);
            }
          });
          
          // Observe changes to the DOM
          observer.observe(document.body, {
            childList: true,
            subtree: true
          });
          
          // Safety timeout - stop observing after 10 seconds if nothing happens
          setTimeout(() => {
            observer.disconnect();
          }, 10000);
        }
    
        // Start the update process
        attemptUpdate();
      });
    </script>
    <!-- Sticky button -->
    <script>
      const clone = document.createElement("a");

      // Set the properties of the clone
      clone.id = "sticky-apply-btn";
      clone.style.position = "fixed";
      clone.style.height = "180px";
      clone.style.width = "52px";
      clone.style.borderTopRightRadius = "10px";
      clone.style.borderBottomRightRadius = "10px";
      clone.style.zIndex = "9999";
      clone.style.bottom = "50%";
      clone.style.right = "0";
      clone.style.backgroundColor = "#005fa9";
      clone.style.boxShadow = "0 4px 8px rgba(0, 0, 0, 0.2)";
      clone.style.translate = "0 25%";

      // Add text and make it sideways
      clone.innerText = "APPLY NOW";
      clone.style.color = "white";
      clone.style.fontWeight = "bold";
      clone.style.fontSize = "18px";
      clone.style.display = "flex";
      clone.style.justifyContent = "center";
      clone.style.alignItems = "center";
      clone.style.textDecoration = "none";

      // This is the key part - rotate the text 90 degrees
      clone.style.writingMode = "vertical-rl";
      clone.style.transform = "rotate(180deg)";
      clone.style.cursor = "pointer";

      //  Adding the href
      clone.href =
        "https://onlinepgdm.imi.edu/?locale=en&program_sfid=01tRC000000b6xiYAA&source=applynowlp";
      clone.target = "_blank";

      document.body.appendChild(clone);
    </script>
    <!-- Form Embed -->
    <script type="text/javascript">
      document.addEventListener("DOMContentLoaded", function () {
        function injectWidget() {
          const formContainer = document.querySelector(".row.items--flex-end");
          if (formContainer) {
            // Remove redundant element
            const redundantElem = document.querySelector(
              ".col-12.right-col.hero_hero__leadFormContainer.col-lg-5"
            );
            if (redundantElem) redundantElem.remove();

            // Create the widget container
            const widgetContainer = document.createElement("div");
            widgetContainer.className = "npfWidgetContainer";

            // Set responsive width based on screen size
            function setResponsiveWidth() {
              if (window.innerWidth <= 991) {
                widgetContainer.style.width = "100%";
              } else {
                widgetContainer.style.width = "41%";
              }
            }

            // Set initial width
            setResponsiveWidth();

            // Add resize listener to adjust width on window resize
            window.addEventListener("resize", setResponsiveWidth);

            // Rest of your existing code
            widgetContainer.style.marginX = "auto";
            widgetContainer.style.translate = "0 -10%";
            widgetContainer.style.height = "400px";
            widgetContainer.style.backgroundColor = "#fff";
            widgetContainer.style.display = "inline-block";

            const widgetDiv = document.createElement("div");
            widgetDiv.className = "npf_wgts";
            widgetDiv.setAttribute("data-height", "400px");
            widgetDiv.setAttribute(
              "data-w",
              "5f18efeb970d645a64e55f82f8662afb"
            );

            // Replace the form with our widget
            widgetContainer.appendChild(widgetDiv);
            formContainer.appendChild(widgetContainer);

            // Load the nopaperforms script
            const s = document.createElement("script");
            s.type = "text/javascript";
            s.async = true;
            s.src = "https://widgets.in6.nopaperforms.com/emwgts.js";
            document.body.appendChild(s);

            return true;
          }
          return false;
        }

        // Same initialization code as before...
        if (!injectWidget()) {
          setTimeout(() => {
            if (!injectWidget()) {
              const observer = new MutationObserver((mutations) => {
                if (injectWidget()) {
                  observer.disconnect();
                }
              });

              observer.observe(document.body, {
                childList: true,
                subtree: true,
              });

              setTimeout(() => observer.disconnect(), 10000);
            }
          }, 1000);
        }
      });
    </script>
    <!-- Replacing Buttons for the new form -->
    <script>
      document.addEventListener("DOMContentLoaded", function () {
        // Initial button replacement
        replaceInitialButtons();

        // Setup observer for dynamic buttons
        setupDynamicButtonObserver();

        // Load NoPaperForms widget scripts
        loadNPFWidgetScripts();

        // Function to replace initial buttons
        function replaceInitialButtons() {
          const buttons = document.querySelectorAll(
            ".btn--container.sticky-brochure_sticky-brochure__btnGroup.branding-primary-reverse .btn.btn--large, .mt-3.btn--container .btn.btn--primary.btn--large"
          );

          buttons.forEach((button) => {
            replaceWithNPFButton(button);
          });
        }

        // Function to set up observer for dynamically appearing button
        function setupDynamicButtonObserver() {
          const observer = new MutationObserver((mutations) => {
            const dynamicButton = document.querySelector(
              ".btn.col-12.info-bar--btn.btn--primary.border.border-1.btn--large"
            );

            if (
              dynamicButton &&
              !dynamicButton.classList.contains("npf-processed")
            ) {
              dynamicButton.classList.add("npf-processed");
              replaceWithNPFButton(dynamicButton);
            }
          });

          // Start observing
          observer.observe(document.body, {
            childList: true,
            subtree: true,
          });

          // Check immediately in case the button is already there
          const existingDynamicButton = document.querySelector(
            ".btn.col-12.info-bar--btn.btn--primary.border.border-1.btn--large"
          );
          if (existingDynamicButton) {
            replaceWithNPFButton(existingDynamicButton);
          }
        }

        // Helper function to replace a button with NPF button
        function replaceWithNPFButton(button) {
          // Create the new button element
          const newButton = document.createElement("button");
          newButton.type = "button";
          newButton.className =
            "npfWidgetButton npfWidget-5f18efeb970d645a64e55f82f8662afb";
          newButton.textContent = "Enquire Now!";

          // Check if this is the sticky brochure button
          const isStickyButton =
            button.closest(".sticky-brochure_sticky-brochure__btnGroup") !==
            null;

          // Set button color based on type
          if (isStickyButton) {
            // Gold color for sticky button
            newButton.style.background =
              "linear-gradient( 0deg, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.2) ), #ff7200";
            newButton.style.color = "#000000";
            newButton.addEventListener("mouseover", function () {
              this.style.boxShadow = "0 4px 8px rgba(0,0,0,0.3)";
              this.style.background =
                "linear-gradient( 0deg, rgba(0, 0, 0, 0.3), rgba(0, 0, 0, 0.3) ), #ff7200";
            });

            newButton.addEventListener("mouseout", function () {
              this.style.boxShadow = "0 2px 5px rgba(0,0,0,0.2)";
              this.style.background =
                "linear-gradient( 0deg, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.2) ), #ff7200";
            });
          } else {
            // Blue color for other buttons
            newButton.style.backgroundColor = "#005fa9";
            newButton.style.color = "#ffffff";

            // Hover effect with darker blue
            newButton.addEventListener("mouseover", function () {
              this.style.backgroundColor = "#003e6e";
              this.style.boxShadow = "0 4px 8px rgba(0,0,0,0.3)";
            });

            newButton.addEventListener("mouseout", function () {
              this.style.backgroundColor = "#005fa9";
              this.style.boxShadow = "0 2px 5px rgba(0,0,0,0.2)";
            });
          }

          // Common styles for both buttons
          newButton.style.padding = "12px 24px";
          newButton.style.borderRadius = "6px";
          newButton.style.fontWeight = "700";
          newButton.style.fontSize = "16px";
          newButton.style.border = "none";
          newButton.style.cursor = "pointer";
          newButton.style.transition = "all 0.3s ease";
          newButton.style.boxShadow = "0 2px 5px rgba(0,0,0,0.2)";
          newButton.style.display = "inline-block";
          newButton.style.textAlign = "center";
          newButton.style.margin = "10px 0";
          newButton.style.width = "auto";
          newButton.style.translate = "50% 0";

          // Replace the old button with the new one
          button.parentNode.replaceChild(newButton, button);
        }

        // Helper function to load NoPaperForms widget scripts
        function loadNPFWidgetScripts() {
          const popupScript1 = document.createElement("script");
          popupScript1.src = "https://in6cdn.npfs.co/js/widget/npfwpopup.js";
          document.body.appendChild(popupScript1);

          popupScript1.onload = function () {
            const popupScript2 = document.createElement("script");
            popupScript2.textContent = `
          let npfW5f18efeb970d645a64e55f82f8662afb = new NpfWidgetsInit({ 
            "widgetId":"5f18efeb970d645a64e55f82f8662afb", 
            "baseurl":"widgets.in6.nopaperforms.com", 
            "formTitle":"Enquiry Form", 
            "titleColor":"#FF0033", 
            "backgroundColor":"#ddd", 
            "iframeHeight":"500px", 
            "buttonbgColor":"#4c79dc", 
            "buttonTextColor":"#FFF" 
          });
        `;
            document.body.appendChild(popupScript2);
          };
        }
      });
    </script>

    <!-- Embed Script -->
    <script type="text/javascript">
      var s = document.createElement("script");
      s.type = "text/javascript";
      s.async = true;
      s.src = "https://widgets.in6.nopaperforms.com/emwgts.js";
      document.body.appendChild(s);
    </script>

    <!-- Popup script -->
    <script src="https://in6cdn.npfs.co/js/widget/npfwpopup.js"></script>
    <script>
      let npfW5f18efeb970d645a64e55f82f8662afb = new NpfWidgetsInit({
        widgetId: "5f18efeb970d645a64e55f82f8662afb",
        baseurl: "widgets.in6.nopaperforms.com",
        formTitle: "Enquiry Form",
        titleColor: "#FF0033",
        backgroundColor: "#ddd",
        iframeHeight: "500px",
        buttonbgColor: "#4c79dc",
        buttonTextColor: "#FFF",
      });
    </script>
  </body>
